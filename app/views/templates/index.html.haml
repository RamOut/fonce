- @templates.each do |template|
  .panel.panel-default
    .panel-body
      = template.name
    .panel-footer
      = template.description



/ %td= link_to 'Edit', edit_template_path(template)
/ %td= link_to 'Destroy', template, :method => :delete, :data => { :confirm => 'Are you sure?' }


= link_to 'New Template', new_template_path, :class=>"btn btn-primary"
